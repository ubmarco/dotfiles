{
  // The first entry is here due to https://github.com/microsoft/vscode/issues/177142
  "json.schemaDownload.enable": true,
  "autoDocstring.docstringFormat": "sphinx",
  "aws.profile": "profile:ub_marco.heinemann",
  "aws.sam.enableCodeLenses": false,
  "compareHelper.defaultExternalTools": {
    "folders": "bcompare",
    "images": "bcompare",
    "text": "bcompare"
  },
  "compareHelper.externalTools": [
    {
      "name": "bcompare",
      "path": "bcompare",
      "compares": ["text", "folders", "images"]
    }
  ],
  "debug.focusWindowOnBreak": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
    "source.fixAll": true
  },
  "editor.fontFamily": "Source Code Pro",
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.largeFileOptimizations": false,
  "editor.mouseWheelZoom": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderWhitespace": "all",
  "editor.rulers": [120],
  "editor.suggestSelection": "first",
  "editor.wordWrapColumn": 120,
  "files.associations": {
    "*.arxml": "xml",
    "*.spec": "python",
    "*.fjson": "json"
  },
  "files.autoSave": "afterDelay",
  "files.exclude": {
    ".history/**": true,
    "**/__pycache__": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.git": false,
    "**/.project": true,
    "**/.settings": true,
    "build/**": true
  },
  "files.watcherExclude": {
    "**/.tox/**": true,
    "**/target/**": true
  },
  "files.insertFinalNewline": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.pullTags": false,
  "git.suggestSmartCommit": false,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.views.commits.files.layout": "tree",
  "gitlens.views.formats.commits.label": "${❰ tips ❱ ➤  }${id} ${message}",
  "importMagic.maxColumns": 120,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "json.schemas": [],
  "local-history.daysLimit": 30,
  "local-history.maxDisplay": 100,
  "local-history.path": "~/.cache/vscode-history",
  "local-history.saveDelay": 5,
