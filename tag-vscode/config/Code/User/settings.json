  "search.exclude": {
    "**/.mypy_cache": true,
    "**/.venv": true,
    "**/data-management-docs": true,
    "**/docs/_build": true,
    "sst/.build": true
  },
  "sort-imports.on-save": false,
  "spellright.documentTypes": [
    "markdown",
    "latex",
    "plaintext",
    "restructuredtext"
  ],
  "spellright.language": ["en_US"],
  "spellright.notificationClass": "warning",
  "spellright.ignoreRegExpsByClass": {
    "restructuredtext": ["/\\\\n/g"]
  },
  "redhat.telemetry.enabled": false,
  "restructuredtext.editor.listEditing.list.indentationSize": "inherit",
  "terminal.integrated.fontFamily": "Source Code Pro",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.titleBarStyle": "custom",
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "always",
  "xmlTools.enableXmlTreeViewCursorSync": true,
  "yaml.customTags": [
    "!GetAtt",
    "!Join sequence",
    "!encrypted/pkcs1-oaep scalar",
    "!vault scalar"
  ],
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.rulers": [100],
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.rulers": [100],
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jinja-html]": {
    "editor.defaultFormatter": "monosans.djlint"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[restructuredtext]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wrappingIndent": "same",
    "editor.wordWrapColumn": 120
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[python]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.formatOnType": true
  },
  "[yaml]": {
    "editor.rulers": [99],
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml",
    "editor.formatOnSave": false
  },
  "search.useIgnoreFiles": false,
  "workbench.startupEditor": "none",
  "workbench.colorTheme": "Visual Studio Dark",
  "hediet.vscode-drawio.resizeImages": null,
  "editor.stickyScroll.enabled": true,
  "window.zoomLevel": -1
}
