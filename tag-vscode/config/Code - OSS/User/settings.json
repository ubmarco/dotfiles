{
  "autoDocstring.docstringFormat": "sphinx",
  "C_Cpp.updateChannel": "Insiders",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.fontFamily": "Source Code Pro",
  "editor.formatOnSave": true,
  "editor.largeFileOptimizations": false,
  "editor.mouseWheelZoom": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.rulers": [120],
  "editor.wordWrapColumn": 120,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.associations": {
    "*.arxml": "xml",
    "*.spec": "python"
  },
  "files.autoSave": "afterDelay",
  "files.exclude": {
    "**/__pycache__": true
  },
  "files.insertFinalNewline": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "importMagic.maxColumns": 120,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "python.formatting.blackArgs": [
    "--line-length",
    "120",
    "--skip-string-normalization",
    "--target-version",
    "py38"
  ],
  "python.formatting.provider": "yapf",
  "python.linting.mypyEnabled": true,
  "python.pythonPath": ".venv/bin/python",
  "python.workspaceSymbols.exclusionPatterns": ["**/site-packages/**", ".venv"],
  "restructuredtext.linter.extraArgs": ["--max-line-length", "120"],
  "restructuredtext.linter.run": "onSave",
  "restructuredtext.updateDelay": 5000,
  "restructuredtext.updateOnTextChanged": "false",
  "terminal.integrated.fontFamily": "Source Code Pro",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "always",
  "xmlTools.enableXmlTreeViewCursorSync": true,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
}
